{
    "name": "laravel-goto-path",
    "displayName": "Laravel Goto Path",
    "description": "Quick jump to path",
    "version": "0.0.4",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-path",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onLanguage:php",
        "onLanguage:blade",
        "onUri",
        "onStartupFinished"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel Goto Path",
            "properties": {
                "laravelGotoPath.list": {
                    "type": "object",
                    "default": {
                        "base_path": "/",
                        "app_path": "/app",
                        "config_path": "/config",
                        "database_path": "/database",
                        "public_path": "/public",
                        "resource_path": "/resources",
                        "storage_path": "/storage",
                        "mix": "/public",
                        "asset": "/public",
                        "global_asset": "/public",
                        "secure_asset": "/public"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "methods to search for"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.23",
        "@types/vscode": "^1.52.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "tslint": "^6.1.3",
        "typescript": "^4.6.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "escape-string-regexp": "^4.0.0",
        "lodash": "^4.17.21"
    }
}
